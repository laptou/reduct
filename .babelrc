{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "usage",
        "corejs": 3,
        "shippedProposals": true
      }
    ],
    ["@babel/typescript", {
      "onlyRemoveTypeImports": true
    }]
  ],
  "plugins": [
    "@babel/proposal-optional-chaining",
    "@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
  ]
}